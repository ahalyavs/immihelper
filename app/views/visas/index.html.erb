<div class="page-header">
  <h1>
    Visas
    <a href="/visas/new" class="btn btn-lg btn-success">New Visa</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Status</th>
        <th>Actions</th>
      </tr>

      <% @visas.each do |visa| %>
      <tr>
        <td><%= visa.status %></td>
        <td>
          <a href="/visas/<%= visa.id %>" class="btn btn-primary">Show</a>
          <a href="/visas/<%= visa.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_visa/<%= visa.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
